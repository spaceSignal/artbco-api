{
  "name": "Artwork",
  "plural": "Artworks",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "display": {
      "type": "boolean",
      "required": true
    },
    "media": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true
    },
    "width": {
      "type": "number",
      "required": true,
      "comments": "width X length X depth"
    },
    "depth": {
      "type": "number",
      "required": true,
      "comments": "width X length X depth"
    },
    "geolocation": {
      "type": "geopoint"
    },
    "serialNumber": {
      "type": "string"
    },
    "edition": {
      "type": "number",
      "required": true
    },
    "editionCount": {
      "type": "number",
      "required": true
    },
    "length": {
      "type": "number",
      "required": true,
      "comments": "width X length X depth"
    },
    "salesPrice": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "images": {
      "type": "hasMany",
      "model": "Image",
      "foreignKey": "imageId"
    },
    "artist": {
      "type": "belongsTo",
      "model": "Artist",
      "foreignKey": "artistId"
    },
    "tags": {
      "type": "hasMany",
      "model": "tag",
      "foreignKey": "tagId"
    },
    "channels": {
      "type": "embedsMany",
      "model": "Channel",
      "property": "channel",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "shippings": {
      "type": "hasMany",
      "model": "Shipping",
      "foreignKey": "shippingId"
    },
    "gallery": {
      "type": "belongsTo",
      "model": "Gallery",
      "foreignKey": "galleryId"
    },
    "stats": {
      "type": "embedsOne",
      "model": "Stats",
      "property": "stats",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
