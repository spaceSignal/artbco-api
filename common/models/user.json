{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "userType": {
      "type": "string",
      "required": true
    },
    "geolocation": {
      "type": "string"
    },
    "pwd": {
      "type": "string"
    },
    "oauthToken": {
      "type": "string"
    },
    "chgPwdOnNext": {
      "type": "string"
    },
    "role": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "address": {
      "type": "embedsOne",
      "model": "Address",
      "property": "address",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "avatar": {
      "type": "embedsOne",
      "model": "Image",
      "property": "avatar",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "user-art-stats": {
      "type": "hasMany",
      "model": "user-art-stats",
      "foreignKey": "artStatsId"
    },
    "user-collections": {
      "type": "hasOne",
      "model": "user-collection",
      "foreignKey": "userId"
    },
    "notifications": {
      "type": "embedsOne",
      "model": "notification",
      "property": "notifications",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "customers": {
      "type": "hasOne",
      "model": "Customer",
      "foreignKey": "userId"
    },
    "conversations": {
      "type": "hasMany",
      "model": "Conversation",
      "foreignKey": "conversationId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
